   -
     name : TransformeDate
     internal_properties : CAAAAAgAAACI4D3xizdqF5aRnC4fcns0++adS7mllwUIOMxUt2Ou45JqRk79kmDYivhREqrT2z185ZfqbZ213PcqdnxMYjJY2y8oDXR5BX3ox0nhnxtv22QvNHLCtWYFnfJ9vfobv568GzzYKUz0L3d6dZZ/mc77YiAmy/VOVXp3JANLKOikkfAiVMi1wu9KIE0//Th8xTSQNVgF7DAmTA38QmAbhtChw3Iatm5r
     procedure_id : 1487575353346182025
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] TransformeDate (<nCentaineNanoSecondes> est entier)
      //
      // Paramètres :
      //	nCentaineNanoSecondes (entier) : Nombre de nanosecondes
      // Valeur de retour :
      // 	variant : Date formaté
      
      Procédure TransformeDate(LOCAL nCentaineNanoSecondes est un entier)
      
      nSecondes est un entier sur 8 octets
      nNbJours est un entier
      dhDateHeure est un dateheure
      
      // Transformation du nombre de centaines de nanosecondes en secondes
      nSecondes = nCentaineNanoSecondes / puissance(10,7)
      
      // On récupère le nombre d'années depuis le 1601
      dhDateHeure..annee = PartieEntière(nSecondes / (60*60*24*365.25))
      
      // On soustrait le nombre d'année rapportées en secondes au nombre de secondes totales et on calcule le nombre de jours
      nSecondes -= dhDateHeure..annee * 60*60*24*365.25
      nNbJours = nSecondes /(60*60*24)
      
      // On soustrait le nombre secondes des Jours et on calcule le nombre d'heure
      nSecondes -= nNbJours * (60*60*24)
      dhDateHeure..heure = nSecondes / 3600
      
      // On soustrait le nombre secondes des heures et on calcule le nombre de minutes
      nSecondes -= dhDateHeure..heure * 3600
      dhDateHeure..minute = nSecondes / 60
      
      // Mise à jour de la date au format dateheure
      dhDateHeure..mois = 1
      dhDateHeure..Année += 1601
      dhDateHeure..jour = 1
      dhDateHeure..jour += nNbJours
      
      
      
      renvoyer chaineconstruit(<§$0000§>,dateverschaine(dhDateHeure..PartieDate,<§$0001§>),heureverschaine(dhDateHeure..partieheure,<§$0002§>))
     type : 458752
   -
     name : MAJPropriétés
     internal_properties : 
     procedure_id : 1487575353346247561
     type_code : 14
     code : |1+
      Procédure MAJPropriétés()
      
      sNoeud est une chaine // Noeud de l'arbre sélectionné dans la fenêtre principale
      sChaineTemp est une chaine // Chaine qui contient l'identifiant de chaque feuille de l'arbre et donc la valeur de la propriété
      sDomaine est une chaine // Nom du domaine parent
      i est un entier = 1 // Compteur
      sDescription est une chaine // Description du groupe
      sNoeudGroupe est une chaine // Noeud de l'arbre des groupes
      nCentaineNanoSecondes est un entier sur 8 octets // Nombre de centaine de nanosecondes écoulés entre le 1/01/1601 à 12 h 00 et l'heure de dernière connexion
      
      // Récupération du noeud sélectionné
      sNoeud = arbreselect(FEN_Principale.ARBRE_Item)
      
      // On concatène le noeud trouvé avec le nom de l'utilisateur
      sNoeud += tab + FEN_Principale.TABLE_ITEM.COL_Valeur[gnLigne]
      
      
      // Récupération du common name
      sChaineTemp  = ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$0003§>)
      LIB_CN = extraitchaine(sChaineTemp, 2, tab)
      
      mafenetre..titre = chaineconstruit(mafenetre..titre,extraitchaine(sChaineTemp, 2, tab))
      
      
      // Récupération du display name
      sChaineTemp = ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$0004§>)
      LIB_DISPLAYNAME = extraitchaine(sChaineTemp, 2, tab)
      
      // Récupération du login
      sChaineTemp = ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$0005§>)
      si sChaineTemp = "" ALORS
      	LIB_INITIALS = <§$0006§>
      sinon	
      	LIB_INITIALS = extraitchaine(sChaineTemp, 2, tab)
      fin
      
      // Récupération des groupes dont cet utilisateur fait partie
      sChaineTemp = ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$0007§>)
      sChaineTemp = extraitchaine(sChaineTemp, 2, tab)
      
      // Extraction du nom de domaine (Commence par la fin)
      sDomaine = extraitchaine(sChaineTemp, 1, <§$0008§>, DepuisFin)
      
      tantque sDomaine <> "" et sDomaine <> eot
      	
      	// Mise à jour du noeud dans lequel il faut ajouter le groupe
      	sNoeudGroupe += sDomaine[[ 4 a]]
      	
      	// Récupération de la description du groupe en question
      	sDescription = extraitchaine(ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeudGroupe + tab + <§$0009§>), 2)
      	
      	// Ajoute le groupe dans l'arbre
      	arbreajoute(ARBRE_GROUPE, sNoeudGroupe, aDéfaut, aDéfaut, sDescription)
      	
      	// Ajoute le TAB à la fin du noeud
      	sNoeudGroupe += tab
      	
      	// Passe au suivant
      	i++
      	sDomaine = extraitchaine(sChaineTemp, i, <§$0008§>, DepuisFin)
      	
      FIN
      
      // Déroule l'arbre
      arbrederouletout(ARBRE_GROUPE)
      arbreselectplus(ARBRE_GROUPE, ARBRE_GROUPE[1])
      executetraitement(ARBRE_GROUPE, trtSélection)
      
      // Récupération de la date de dernière connexion
      sChaineTemp = ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$000a§>)
      
      
      // Récupération du nombre de centaine de nanosecondes écoulée depuis le 1 janvier 1601 à 12 h 00
      nCentaineNanoSecondes = val(extraitchaine(sChaineTemp, 2))
      si nCentaineNanoSecondes > 0 ALORS
      	LIB_LASTCONNEXION = TransformeDate(nCentaineNanoSecondes) 	
      sinon
      	LIB_LASTCONNEXION = <§$000b§>
      FIN
      
      
      
      // Récupération de la propriété Admin
      sChaineTemp =  ArbreRécupèreIdentifiant(FEN_Principale.ARBRE_Cache, sNoeud +tab+ <§$000c§>)
      INT_ADMIN = val(extraitchaine(sChaineTemp, 2))
     type : 458752
