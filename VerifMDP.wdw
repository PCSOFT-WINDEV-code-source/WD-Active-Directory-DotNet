   -
     name : RemplitUserList
     internal_properties : 
     procedure_id : 1486210391411229182
     type_code : 14
     code : |1-
      // Syntaxe :
      //RemplitUserList ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure RemplitUserList()
      
      // Variables locales
      clRechercheDirectory est un DirectorySearcher(gclObjDirEnt) // Objet .NET DirectorySearcher qui permet de réaliser des recherches dans l'active Directory
      pclListeRésultat est un SearchResultCollection dynamique // Objet .NET SearchResultCollection qui permet de récupérer tous les résultats d'une recherche
      pclRésultat est un SearchResult dynamique // Objet .NET SearchResult contenant UN résultat de recherche
      sNomUser est une chaine
      
      //Recherche est un DirectorySearcher dynamique
      clRechercheDirectory:Filter = "(objectClass=user)"
      
      
      quand Exception dans
      	// Récupération des informations
      	pclListeRésultat = clRechercheDirectory:FindAll()
      faire
      	// Message d'erreur
      	erreur(<§$0002§> + gsDomaine,<§$0003§>)
      	// Ferme la fenêtre
      	ferme()
      fin
      // Si aucun résultat
      si pclListeRésultat:count = 0 ALORS
      	// Message d'erreur
      	erreur(<§$0004§>,<§$0005§>)
      	// Ferme la fenêtre
      	ferme()
      FIN
      
      // Borne max
      nBorne	est un entier = pclListeRésultat:Count - 1
      // Parcours des résultats
      pour i = 0 a nBorne
      	
      	// Récupère l'objet .NET SearchResult
      	pclRésultat = pclListeRésultat:get_item(i)
      	// On extrait son nom
      	sNomUser = extraitchaine(pclRésultat:path, 1, ",")
      	// Définit le chemin
      	sNomUser = remplace(sNomUser, "LDAP://" + gsDomaine + "/CN=", "")
      	// Recherche avec ce chemin
      	clRecherche est un directoryEntry (pclRésultat:path)
      	// Ajoute le nom à la liste
      	listeajoute(COMBO_UTILISATEUR, glien(pclRésultat:path) + sNomUser)
      	
      FIN
      
      // Trie la liste par ordre croissant de nom d'utilisateur
      listetrie(COMBO_UTILISATEUR, vrai)
